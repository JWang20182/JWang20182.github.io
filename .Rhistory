q13_2 <- matrix(c(1.4, 0.4,   # Odds for Program A
1.0, 1.6,   # Odds for Program B
2.0, 1.142857143), # Odds for Program C
nrow = 3, byrow = TRUE)
rownames(q13_2) <- c("A", "B", "C")
colnames(q13_2) <- c("Private", "Public")
# Create the contingency tables for the three programs
program_a <- matrix(c(35, 25, 40, 100), nrow = 2, byrow = TRUE)
program_b <- matrix(c(10, 10, 80, 50), nrow = 2, byrow = TRUE)
program_c <- matrix(c(50, 25, 40, 35), nrow = 2, byrow = TRUE)
# Perform the chi-square test for each program
result_a <- chisq.test(program_a)
result_b <- chisq.test(program_b)
result_c <- chisq.test(program_c)
# Print the results
cat("Program A: p-value =", result_a$p.value, "\n")
cat("Program B: p-value =", result_b$p.value, "\n")
cat("Program C: p-value =", result_c$p.value, "\n")
